                </div>
                <div class="experience_filters" data-behaviour="experiencelist_filters">
                    <select class="experience_filter type" data-behaviour="experiencelist_filter" data-filtertype="type">
                        <option value="all">All Types</option>
                    </select>
                    <select class="experience_filter state" data-behaviour="experiencelist_filter" data-filtertype="state">
                        <option value="all">All States</option>
                    </select>
                    <select class="experience_filter sort" data-behaviour="experiencelist_filter" data-filtertype="sort">
                        <option value="default">Sort by State</option>
                        <option value="title">Sort by Title</option>
                        <option value="type">Sort by Type</option>
                        <option value="logcount">Sort by LogCount</option>
                        <option value="rating">Sort by Rating</option>
                        <!--<option value="distance">Sort by Distance</option>-->
                    </select>
                </div>
            </div>

            <!--INFO-->
            <div class="section_part section_location_info">
                <input type="radio" id="part_info" name="partselector">
                <div class="location_page numbers" data-behaviour="infoPage">
                    <!--Generate Info with JS-->
                </div>
            </div>

            <!--CHARACTERS-->
            <div class="section_part section_location_characters">
                <input type="radio" id="part_characters" name="partselector">
                <div class="location_page location_characters">
                    <div class="characters_list" data-behaviour="characterList">
                        <!--Generate character list with JS-->
                    </div>
                </div>
            </div>

            <!--FOOD-->
            <div class="section_part section_location_food">
                <input type="radio" id="part_food" name="partselector">
                <div class="location_page location_food">
                    <div class="food_list" data-behaviour="foodList">
                        <!--Generate food list with JS-->
                    </div>
                </div>
            </div>

            <!--AREAS-->
            <div class="section_part section_location_areas">
                <input type="radio" id="part_areas" name="partselector">
                <div class="location_page location_areas">
                    <div class="areas_list" data-behaviour="areaList">
                        <!--Generate area list with JS-->
                    </div>
                </div>
            </div>

            <!--EXPERIENCES-->
            <div class="section_part section_location_experiences">
                <input type="radio" id="part_experiences" name="partselector" checked="checked">
                <div class="location_page location_experiences">
                    <div class="experience_list" data-behaviour="experienceList" data-parkID="$ID">
                        <!--Generate experience list with JS-->
                    </div>
                </div>
            </div>

            <!--MAP
            <div class="section_part section_location_map">
                <input type="radio" id="part_map" name="partselector">
                <h2>Coming Soon</h2>
            </div>-->
        </div>
    </div>
<% end_with %>
