<div class="loggedexperience_card">
    <div class="log_text">
        <p>$FormattedDate</p>
        <h3>$Experience.Title</h3>
        <div class="seemore">
            <% if $Train >= 0 %><p>Train: $Train</p><% end_if %>
            <% if $Wagon >= 0 %><p>Wagon: $Wagon</p><% end_if %>
            <% if $Row >= 0 %><p>Row: $Row</p><% end_if %>
            <% if $Seat >= 0 %><p>Seat: $Seat</p><% end_if %>
            <p>$Notes</p>
        </div>
    </div>
    <div class="log_weather">
